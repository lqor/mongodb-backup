    "uniqueTest": "let CONSOLE_LOG_CALLED = false;\r\nconst ORIGINAL_CONSOLE_LOG = console.log;\r\n\r\nconsole.log = function (...args) {\r\n    CONSOLE_LOG_CALLED = true;\r\n    ORIGINAL_CONSOLE_LOG.apply(console, args);\r\n};\r\n\r\nif (typeof sum !== \"function\") {\r\n    testResults.push({\r\n        testRef: \"TEST_ID\",\r\n        success: false,\r\n        message: \"Function sum is not defined\"\r\n    });\r\n} else {\r\n    try {\r\n        sum(2, 3);\r\n\r\n        if (CONSOLE_LOG_CALLED) {\r\n            testResults.push({\r\n                testRef: \"TEST_ID\",\r\n                success: true,\r\n                message: \"console.log was called\"\r\n            });\r\n        } else {\r\n            testResults.push({\r\n                testRef: \"TEST_ID\",\r\n                success: false,\r\n                message: \"console.log was NOT called\"\r\n            });\r\n        }\r\n    } catch (e) {\r\n        testResults.push({\r\n            testRef: \"TEST_ID\",\r\n            success: false,\r\n            message: \"Runtime error: \" + e\r\n        });\r\n    }\r\n}"
